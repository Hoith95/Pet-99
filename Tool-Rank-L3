local function runScript2()
    -- Chờ đến khi script 1 hoàn tất
    while not script1Done do
        print("Chờ script 1 hoàn thành...")
        task.wait(1) -- Dừng trong 1 giây trước khi kiểm tra lại
    end
    print("Bắt đầu chạy script 2...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Pira-Holiday-Event"))()
    print("Script 2 đã chạy xong.")
end
