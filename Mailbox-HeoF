_G.script1Done = true
_G.Time_Trial_Var = true

--- Gửi Mail ---
spawn(function()
wait(15)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Mailbox-Heo"))()
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Mastery"))()

loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/VAR-RESET"))()

spawn(function()
    local targetPlaceId = 140403681187145
    if game.PlaceId == targetPlaceId then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Monitor15Min"))()
    else
        warn("⛔ Không đúng PlaceId, bỏ qua script.")
    end
end)

spawn(function()
    local targetPlaceId = 140403681187145
    if game.PlaceId ~= targetPlaceId then
        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("World4Teleport"):InvokeServer()
    else
        warn("⛔ Không đúng PlaceId, bỏ qua script.")
    end
end)

