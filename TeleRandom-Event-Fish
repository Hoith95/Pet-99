-- Kiểm tra biến toàn cục để xác định script đã chạy hay chưa
if _G.TELEEVFISH then
    warn("Script đã được chạy trước đó!")
    return -- Dừng script nếu đã chạy
end

-- Đánh dấu script đã được chạy
_G.TELEEVFISH = true

-- Gọi thuyền
local args = {
    [1] = "Boat 1"
}
game:GetService("ReplicatedStorage").Network.Boats_RequestSpawn:InvokeServer(unpack(args))

wait(10)

-- Danh sách các vị trí có thể dịch chuyển
local positions = {
    CFrame.new(11728, 4.16, 212),
    CFrame.new(12339, 4.16, 456),
    CFrame.new(12786, 4.16, -377),
    CFrame.new(11974, 4.16, -159)
}

-- Dịch chuyển đến một vị trí ngẫu nhiên
local randomIndex = math.random(1, #positions)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = positions[randomIndex]
