_G.script2Done = true
_G.Events = true

spawn(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Del-TopBarApp"))()
end)

spawn(function()
wait(68)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Noti-Default"))()
end)

spawn(function()
wait(68)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Noti-Updater2"))()
end)

spawn(function()
game:GetService("ReplicatedStorage").Network:FindFirstChild("Login Streaks: Claim"):InvokeServer()
end)

spawn(function()
    local eventId = game.PlaceId

    if eventId == 119454325063278 then
        -- Farming world event
        getgenv().GDO_FARMING_WORLD = true
        getgenv().GEVENT_UPGRADES = {
            "FarmingHugeChance",
            "FarmingTitanicChance",
            "FarmingPixelRainbowLuck",
            "FarmingGoldHatchChance",
            "FarmingRainbowHatchChance",
            "FarmingShinyHatchChance"
        }
        print("✅ Config: Farming world set")

    elseif eventId == 95635359880599 then
        -- Fishing event
        getgenv().GDO_FISHING_EVENT = true
        getgenv().GUSE_BOOSTS = true
        print("✅ Config: Fishing event set")

    elseif eventId == 140403681187145 then
        -- World 4: check _G.Time_Trial_End một lần
        if _G.Time_Trial_End == true then
            getgenv().GDO_BLOCK_PARTY = true
            getgenv().GBLOCK_PARTY_STOP_REBIRTHING_ON_TITANIC = true
            getgenv().GBLOCK_PARTY_STOP_REBIRTHING_ON_MULTIPLIER2 = 25
            getgenv().GBLOCK_PARTY_MAKE_TROPICAL_GIFTS = true
			spawn(function()
			wait(68)
        	loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-2"))()
			end)
            print("✅ Config: Block Party set (Time_Trial_End)")
        else
            warn("⛔ _G.Time_Trial_End chưa true — set Block Party.")
			spawn(function()
			wait(68)
        	loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
			end)
        end

    else
        warn("⛔ Không phải event được hỗ trợ — không set gì.")
    end
end)

