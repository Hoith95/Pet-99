spawn(function()
    local eventId = game.PlaceId
    if eventId == 119454325063278 then
        -- Farming world event
        getgenv().GDO_FARMING_WORLD = true
        getgenv().GEVENT_UPGRADES = {
            "FarmingHugeChance",
            "FarmingTitanicChance",
            "FarmingPixelRainbowLuck",
            "FarmingGoldHatchChance",
            "FarmingRainbowHatchChance",
            "FarmingShinyHatchChance"
        }
		getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
        print("✅ Config: Farming world set")

    elseif eventId == 95635359880599 then
        -- Fishing event
        getgenv().GDO_FISHING_EVENT = true
        getgenv().GUSE_BOOSTS = true
		getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
        print("✅ Config: Fishing event set")

    elseif eventId == 140403681187145 then
        -- World 4: check _G.Time_Trial_End một lần
        if _G.Time_Trial_End == true then
			_G.Time_Trial_Var = true
			getgenv().GDO_XMAS_WORLD = true
			getgenv().GEVENT_UPGRADES = {
				"MegaPresentGrowFaster",
				"MegaPresentMoreLoot",
				"MegaPresentEggLuckBoost",
				"MegaPresentHigherHugeChance",
				"MegaPresentHigherTitanicChance",
				"MegaPresentHigherGargantuanChance"
			}
			getgenv().GXMAS_WORLD_DO_ICE_BLOCKS = true
			getgenv().GXMAS_WORLD_MIN_HATCH_REQUESTS_PER_CYCLE = 68
			getgenv().GXMAS_WORLD_MIN_FARM_TIME_SECONDS = 300
			getgenv().GXMAS_WORLD_CRAFT_SANTA_GIFTS = true
			getgenv().GDO_LOCKPICKING = true
			getgenv().GUSE_DIAMOND_LOCKPICK = true
			getgenv().GDO_ADVENT_CALENDAR = true
			getgenv().GXMAS_WORLD_GRINCH_SPRAY_THRESHOLD = 10
			getgenv().GXMAS_WORLD_DO_GINGERBREAD_CHEST = true
			getgenv().GXMAS_WORLD_DO_SNOWMAN_BOSS_CHEST = true
			getgenv().GXMAS_WORLD_SPECIFIC_EGG_TO_HATCH = "Present Egg"
			--getgenv().GXMAS_WORLD_PET_ORDER_GOOD_TO_BAD = {"Krampus", "Hippomint", "Candycane Kitsune", "Elf Golem"}
			getgenv().GXMAS_WORLD_AUTO_CRAFT = false
			getgenv().GXMAS_WORLD_FOCUS_EGGS_FOR_AUTO_CRAFT_PETS = false
			getgenv().GXMAS_WORLD_DO_MEGA_PRESENT = false
			--getgenv().GXMAS_WORLD_OPEN_MEGA_PRESENT_IF_LUCK_REACH = 8000
			--getgenv().GXMAS_WORLD_PETS_EXCLUDED_FROM_SENDING = {"Elf Golem", "Candycane Kitsune", "Hippomint", "Krampus","Gingerbread Angelus", "Holiday Owl", "Snowflake Dragon", "Merry Mule"}
			spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/OPTIMIZE_XMASWORLD"))()
			end)
			spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/VAR_EGGS_HATCHED"))()
			end)
			spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event_XMAS_MEGA_PRESENT"))()
			end)
			spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event_XMAS_SECRET_SANTA"))()
			end)
			spawn(function()
			--loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event_XMAS_CRAFT"))()
			end)
			getgenv().GENCHANTS = {"Criticals","Tap Power","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
			spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
			end)
            print("✅ Config: Halloween set (Time_Trial_End)")
        else
            warn("⛔ _G.Time_Trial_End chưa true — set Block Party.")
			getgenv().GENCHANTS = {"Criticals","Criticals","Tap Power","Tap Power","Strong Pets","Super Lightning"}
			spawn(function()
			wait(68)
        	loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
			end)
        end
    else
        warn("⛔ Không phải event được hỗ trợ — không set gì.")
    end
end)

