_G.script2Done = true
_G.Event = true

spawn(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Del-TopBarApp"))()
end)

spawn(function()
wait(68)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Noti-Default"))()
end)

spawn(function()
wait(68)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Noti-Updater2"))()
end)

spawn(function()
game:GetService("ReplicatedStorage").Network:FindFirstChild("Login Streaks: Claim"):InvokeServer()
end)

spawn(function()
    local eventId = game.PlaceId

    if eventId == 119454325063278 then
        -- Farming world event
        getgenv().GDO_FARMING_WORLD = true
        getgenv().GEVENT_UPGRADES = {
            "FarmingHugeChance",
            "FarmingTitanicChance",
            "FarmingPixelRainbowLuck",
            "FarmingGoldHatchChance",
            "FarmingRainbowHatchChance",
            "FarmingShinyHatchChance"
        }
		getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
        print("✅ Config: Farming world set")

    elseif eventId == 95635359880599 then
        -- Fishing event
        getgenv().GDO_FISHING_EVENT = true
        getgenv().GUSE_BOOSTS = true
		getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
        print("✅ Config: Fishing event set")

    elseif eventId == 131952481663528 then
        -- Halloween event
        getgenv().GDO_HALLOWEEN_WORLD = true
		getgenv().GEVENT_UPGRADES = {
		  "HalloweenCandyMultiplier",
		  "HalloweenEggLuck",
		  "HalloweenTrickOrTreatLuck",
		  "HalloweenMoreDiamonds",
		  "HalloweenMoreWitchHats",
		  "HalloweenHugeLuck",
		  "HalloweenTitanicLuck"
		}
		getgenv().GHALLOWEEN_WORLD_BUY_FROM_SPECIFIC_HOUSE = 1
		getgenv().GHALLOWEEN_WORLD_BUY_FROM_HOUSES_BELOW = true
		getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
		spawn(function()
		wait(38)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
		end)
		print("✅ Config: Halloween event set")
		
    elseif eventId == 140403681187145 then
        -- World 4: check _G.Time_Trial_End một lần
        if _G.Time_Trial_End == true then
			_G.GEVENT_FPS  = 5
			getgenv().GDO_HALLOWEEN_WORLD = true
			getgenv().GEVENT_UPGRADES = {
				"HalloweenCandyMultiplier",
				"HalloweenEggLuck",
				"HalloweenTrickOrTreatLuck",
				"HalloweenMoreDiamonds",
				"HalloweenMoreWitchHats",
				"HalloweenHugeLuck",
				"HalloweenTitanicLuck"
			}
			getgenv().GHALLOWEEN_WORLD_BUY_FROM_HOUSES_BELOW = true
			getgenv().GHALLOWEEN_WORLD_BUY_FROM_SPECIFIC_HOUSE = {1,2,3}
			getgenv().GHALLOWEEN_WORLD_HATCH_SPOTS = 1
			getgenv().GHALLOWEEN_WORLD_RECIPES_TO_DO = {0}
			getgenv().GHALLOWEEN_JOIN_LOCAL = true
			getgenv().GENCHANTS = {"Lucky Eggs","Lucky Eggs","Lucky Eggs","Huge Hunter","Treasure Hunter","Diamonds"}
			spawn(function()
        	loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
			end)
            print("✅ Config: Block Party set (Time_Trial_End)")
        else
            warn("⛔ _G.Time_Trial_End chưa true — set Block Party.")
			getgenv().GENCHANTS = {"Criticals","Criticals","Tap Power","Tap Power","Strong Pets","Strong Pets"}
			spawn(function()
			wait(68)
        	loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Event-TimeTrial-3"))()
			end)
        end

    else
        warn("⛔ Không phải event được hỗ trợ — không set gì.")
    end
end)

getgenv().GLOOTBOXES = {"Locked Hype Egg", "Deep Sea Treasure Chest"}

getgenv().GPROGRESS_MODE = "Hybrid" -- Hatch
