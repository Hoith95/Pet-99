--game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("World1Teleport"):InvokeServer()

spawn(function()
    while true do
        wait(360)  -- chờ 360 giây (6 phút)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Mailbox-Suu"))()
    end
end)

spawn(function()
    for i = 1, 5 do
        -- Từ lần thứ 2 trở đi mới kiểm tra số lượng người chơi
        if i > 1 then
            local success, playerCount = pcall(function()
                return #game:GetService("Players"):GetPlayers()
            end)

            if success and playerCount >= 2 and playerCount <= 5 then
                warn("⛔ Server hiện có từ 2 đến 5 người. Dừng chạy script.")
                break
            end
        end

        -- Thực thi script
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/ServerHop-Event-Fish"))()

        -- Nếu chưa đến lần cuối thì đợi 30 giây
        if i < 5 then
            wait(30)
        end
    end
end)

spawn(function()
wait(30)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Mailbox-Huge-Suu"))()
end)

spawn(function()
wait(30)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Notifier"))()
end)

spawn(function()
wait(183)
--loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/LookFPS-Suu"))()
end)

