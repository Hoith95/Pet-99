-- HÃ m trang bá»‹ danh sÃ¡ch pet cÃ³ in log
local function equipPets(petList)
    for _, pet in ipairs(petList) do
        task.wait(0.1)
        print("ğŸ§¸ Trang bá»‹: " .. pet.name .. " x" .. pet.count)
        for i = 1, pet.count do
            Network.Pets_Favorite:FireServer(pet.id)
        end
    end
end

-- Trang bá»‹ Huge trÆ°á»›c
print("ğŸš€ Báº¯t Ä‘áº§u trang bá»‹ Huge Pets...")
equipPets(hugePets)

-- Trang bá»‹ pet thÆ°á»ng sau
print("ğŸ Trang bá»‹ cÃ¡c pet thÆ°á»ng...")
equipPets(regularPets)

