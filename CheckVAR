-- Đặt thời gian chờ là 3 phút (180 giây)
local waitTime = 180

local fishingLocation = game:GetService("Workspace").__THINGS.__INSTANCE_CONTAINER.Active:FindFirstChild("AdvancedFishing")
local player = game.Players.LocalPlayer

local function CheckAndServerHop()
    if fishingLocation then
        print("Đã ở trong AdvancedFishing. Kết thúc script.")
        return
    else
        print("Chưa ở trong AdvancedFishing. Thực hiện ServerHop.")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/ServerHop"))()
    end
end

spawn(function()
    wait(waitTime)
    CheckAndServerHop()
end)
