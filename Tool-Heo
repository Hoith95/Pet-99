local Save = require(game:GetService("ReplicatedStorage").Library.Client.Save)

-- Biến kiểm tra rank
_G.Rank33 = nil  

-- Hàm kiểm tra rank
local function CheckRank()
    local rank = Save.Get().Rank
    if rank >= 33 then
        _G.Rank33 = true
    else
        _G.Rank33 = false
    end
end

-- Chạy kiểm tra rank ngay sau khi script khởi động
CheckRank()

-- Chạy script tương ứng
if _G.Rank33 == true then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Pira-Events"))()
    print("Đang chạy Script 1 vì Rank >= 33")

elseif _G.Rank33 == false then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Tool-HeoF"))()
    print("Đang chạy Script 2 vì Rank < 33")

else
    -- Kiểm tra sau 60 giây nếu _G.Rank33 vẫn chưa xác định
    task.wait(60)
    if _G.Rank33 == nil then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Hoith95/Pet-99/main/Tool-HeoF"))()
        print("Không thể xác định Rank, chạy Script 3")
    end
end
