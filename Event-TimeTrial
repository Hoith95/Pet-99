if _G.VAR_TIME_TRIAL then
	warn("Script Ä‘Ã£ Ä‘Æ°á»£c cháº¡y trÆ°á»›c Ä‘Ã³!")
	return
end

local function RunTimeTrialScript()
	_G.VAR_TIME_TRIAL = true

	task.spawn(function()
		if game.PlaceId ~= 140403681187145 then return end

		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local Save = require(game.ReplicatedStorage.Library.Client.Save)
		local ZoneCmds = require(game.ReplicatedStorage.Library.Client.ZoneCmds)

		local function teleportTo(part)
			if part and part.CFrame then
				local character = player.Character or player.CharacterAdded:Wait()
				if character and character:FindFirstChild("HumanoidRootPart") then
					character.HumanoidRootPart.CFrame = part.CFrame + Vector3.new(0, 5, 0)
				end
			end
		end

		local BreakablesFolder = workspace.__THINGS.Breakables
		local LeaveTeleport = workspace.__THINGS.Instances.TimeTrial.Teleports.Leave

		local function isInsideXRegion()
			local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
			if not hrp then return false end
			local x = hrp.Position.X
			return x >= -18425 and x <= -18415
		end

		local function isValidBreakable(instance)
			return instance:IsA("Model") and tonumber(instance.Name) ~= nil
		end

		local function getCurrentBreakables()
			local list = {}
			for _, child in ipairs(BreakablesFolder:GetChildren()) do
				if isValidBreakable(child) then
					table.insert(list, child.Name)
				end
			end
			return list
		end

		local function waitUntilBreakableGone(name)
			while true do
				local breakable = BreakablesFolder:FindFirstChild(name)
				if not breakable then break end
				task.wait(0.1)
			end
		end

		local function teleportToBreakables()
			while true do
				local breakableNames = getCurrentBreakables()
				if #breakableNames > 0 then
					for _, name in ipairs(breakableNames) do
						local breakable = BreakablesFolder:FindFirstChild(name)
						if breakable and breakable:IsA("Model") then
							local part = breakable:FindFirstChildWhichIsA("BasePart")
							if part then
								teleportTo(part)
								waitUntilBreakableGone(name)
							end
						end
					end
				else
					local hasNew = false
					for i = 1, 20 do
						task.wait(1)
						if #getCurrentBreakables() > 0 then
							hasNew = true
							break
						end
					end

					if not hasNew then
						if LeaveTeleport then
							teleportTo(LeaveTeleport)
							task.wait(5)
						end
						break
					end
				end
				task.wait(0.5)
			end
		end

		local function getTimeTrialRuns()
			local saveData = Save.Get()
			return saveData and saveData.TimeTrialStats and (saveData.TimeTrialStats.Daily_run or 0) or 0
		end

		-- ğŸ” Theo dÃµi tiáº¿n Ä‘á»™ song song
		task.spawn(function()
			local currentRuns = getTimeTrialRuns()
			print("ğŸ“Š Sá»‘ lÆ°á»£t Time Trial Ä‘Ã£ hoÃ n thÃ nh: " .. currentRuns)

			if currentRuns >= 10 then
				warn("ğŸ¯ ÄÃ£ Ä‘á»§ 10 lÆ°á»£t, khÃ´ng cáº§n theo dÃµi thÃªm.")
				return
			end

			while currentRuns < 10 do
				local startRuns = currentRuns
				local totalTime = 0

				print("ğŸ•’ Báº¯t Ä‘áº§u theo dÃµi 12 phÃºt tá»« lÆ°á»£t thá»© " .. startRuns)

				while totalTime < 686 do
					task.wait(10)
					totalTime += 10

					currentRuns = getTimeTrialRuns()
					if currentRuns >= 10 then
						warn("ğŸ‰ ÄÃ£ Ä‘áº¡t 10 lÆ°á»£t trong khi theo dÃµi, káº¿t thÃºc.")
						return
					elseif currentRuns > startRuns then
						print("âœ… LÆ°á»£t tÄƒng (tá»« " .. startRuns .. " â†’ " .. currentRuns .. "), theo dÃµi láº¡i tá»« Ä‘áº§u.")
						break
					end
				end

				if currentRuns == startRuns then
					player:Kick("â° 12 phÃºt rá»“i mÃ  khÃ´ng tÄƒng lÆ°á»£t Time Trial. CÃ³ thá»ƒ bá»‹ AFK hoáº·c lá»—i.")
					return
				end
			end
		end)

		local savedPosition = nil
		local _, maxZoneData = ZoneCmds.GetMaxOwnedZone()
		local requiredZoneNumber = maxZoneData and maxZoneData.ZoneNumber or 0

		local function waitUntilReachZone(targetZone)
			while true do
				local _, currentZoneData = ZoneCmds.GetMaxOwnedZone()
				if currentZoneData and currentZoneData.ZoneNumber >= targetZone then
					print("âœ… ÄÃ£ Ä‘áº¡t Zone " .. targetZone .. ", báº¯t Ä‘áº§u cháº¡y Time Trial.")
					break
				end
				print("â³ ChÆ°a Ä‘áº¡t Zone " .. targetZone .. ", Ä‘ang Ä‘á»£i...")
				task.wait(5)
			end
		end

		waitUntilReachZone(requiredZoneNumber)

		while true do
			local Daily_run = getTimeTrialRuns()
			if Daily_run >= 10 then
				warn("ğŸ¯ ÄÃ£ Ä‘á»§ 10 lÆ°á»£t cháº¡y, dá»«ng script.")
				break
			end

			warn("ğŸ” Báº¯t Ä‘áº§u vÃ²ng Time Trial thá»© " .. tostring(Daily_run + 1))

			local character = player.Character or player.CharacterAdded:Wait()
			local hrp = character:FindFirstChild("HumanoidRootPart")
			if hrp then
				savedPosition = hrp.CFrame
			end

			local failedAttempts = 0
			local MAX_ATTEMPTS = 3
			local successEnter = false

			while failedAttempts < MAX_ATTEMPTS do
				teleportTo(workspace.__THINGS.Instances.TimeTrial.Teleports.Enter)
				task.wait(5)
				warn("ğŸšª ÄÃ£ teleport vÃ o Time Trial, Ä‘ang chá» vÃ¹ng X...")

				-- â³ Äá»£i vÃ o vÃ¹ng X
				local timeout = tick() + 10
				while not isInsideXRegion() and tick() < timeout do
					task.wait(0.5)
				end

				if not isInsideXRegion() then
					failedAttempts += 1
					warn("âš ï¸ KhÃ´ng vÃ o vÃ¹ng X trong 10s (láº§n thá»­ " .. failedAttempts .. ")")
					if LeaveTeleport then
						teleportTo(LeaveTeleport)
						task.wait(3)
					end
					task.wait(5)
					continue
				end

				-- ğŸ•’ Báº¯t Ä‘áº§u Ä‘áº¿m 30s sau khi vÃ o vÃ¹ng X
				task.wait(30)
				if isInsideXRegion() then
					failedAttempts += 1
					warn("âŒ NhÃ¢n váº­t Ä‘á»©ng trong vÃ¹ng X > 30s (láº§n thá»­ " .. failedAttempts .. ")")
					if LeaveTeleport then
						teleportTo(LeaveTeleport)
						task.wait(3)
					end
					task.wait(30)
					continue
				end

				-- âœ… Kiá»ƒm tra Breakables xuáº¥t hiá»‡n
				local foundBreakables = false
				for i = 1, 60 do
					if #BreakablesFolder:GetChildren() > 0 then
						foundBreakables = true
						break
					end
					task.wait(0.1)
				end

				if foundBreakables then
					warn("âœ… Breakables Ä‘Ã£ xuáº¥t hiá»‡n, báº¯t Ä‘áº§u farm")
					successEnter = true
					break
				else
					failedAttempts += 1
					warn("âŒ KhÃ´ng tháº¥y Breakables (láº§n thá»­ " .. failedAttempts .. ")")
					if LeaveTeleport then
						teleportTo(LeaveTeleport)
						task.wait(3)
					end
					task.wait(30)
				end
			end

			if not successEnter then
				player:Kick("TÃ¬m Breakables MÃ©o Tháº¥y")
				return
			end

			teleportToBreakables()
			task.wait(10)

			if LeaveTeleport then
				teleportTo(LeaveTeleport)
				warn("ğŸšª ÄÃ£ ra khá»i Time Trial")
				task.wait(3)
			end

			if savedPosition then
				teleportTo({CFrame = savedPosition})
				warn("ğŸ”™ Quay vá» vá»‹ trÃ­ ban Ä‘áº§u")
			end

			task.wait(15)
		end

		warn("ğŸ‰ ÄÃ£ hoÃ n táº¥t Time Trial hoáº·c Ä‘áº¡t 10 lÆ°á»£t.")

		-- ğŸ•’ Theo dÃµi reset sau 5 phÃºt/láº§n
		task.spawn(function()
			warn("â³ Theo dÃµi reset Daily_run sau 24h...")
			while true do
				task.wait(300)
				local newRun = getTimeTrialRuns()
				if newRun == 0 then
					warn("ğŸ”„ Daily_run Ä‘Ã£ reset vá» 0 â†’ cháº¡y láº¡i toÃ n bá»™ script!")
					_G.VAR_TIME_TRIAL = nil
					RunTimeTrialScript()
					break
				else
					print("ğŸ•“ Daily_run chÆ°a reset, tiáº¿p tá»¥c Ä‘á»£i...")
				end
			end
		end)
	end)
end

RunTimeTrialScript()
