if _G.VAR_TELE_XMAS then return end
_G.VAR_TELE_Xmas = true

wait(15)

local function inXmasWorld()
    local inst = workspace:FindFirstChild("__THINGS")
        and workspace.__THINGS:FindFirstChild("__INSTANCE_CONTAINER")
        and workspace.__THINGS.__INSTANCE_CONTAINER:FindFirstChild("Active")
    return inst and inst:FindFirstChild("XmasWorld") ~= nil
end

local player = game.Players.LocalPlayer

if not inXmasWorld() then
    player:Kick("B·∫°n kh√¥ng ·ªü Xmas World!")
    return
end

print("üéÉ ƒêang ·ªü Xmas Instance ‚Äî D·ª™NG SCRIPT.")
return
